<h1>Show Videos</h1>

<ul>

  <li>
    <strong>Titulo:</strong>
    <%= @videos.titulo %>
  </li>

  <li>
    <strong>Assunto:</strong>
    <%= @videos.assunto %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @videos.url %>
  </li>

  <li>
    <strong>Tags:</strong>
    <%= @videos.tags %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.videos_path(@conn, :edit, @videos) %></span> |
<span><%= link "Back", to: Routes.videos_path(@conn, :index) %></span>
