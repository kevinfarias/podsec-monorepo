{
    "name": "teinformatech-backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "test": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":\"50\",\"branches\":\"50\",\"functions\":\"50\",\"lines\":\"50\"}}'",
        "test:watch": "NODE_ENV='test' npm run db:up && jest --watchAll --coverage --coverageThreshold='{\"global\":{\"statements\":\"50\",\"branches\":\"50\",\"functions\":\"50\",\"lines\":\"50\"}}'",
        "db:create": "npx sequelize db:create || exit 0",
        "db:migrate": "npx sequelize db:migrate",
        "db:seed": "npx sequelize db:seed:all",
        "db:up": "npm run db:create && npm run db:migrate && npm run db:seed",
        "start": "npm run db:up && nodemon -L --exec 'node --experimental-specifier-resolution=node' ./src/bin/www"
    },
    "type": "module",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^16.0.0",
        "express": "~4.16.1",
        "express-rate-limit": "^6.3.0",
        "http-status-codes": "^2.2.0",
        "ioredis": "^4.28.5",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "~1.9.1",
        "pg": "^8.7.1",
        "pg-hstore": "^2.3.4",
        "rate-limiter-flexible": "^2.3.6",
        "redis": "^4.0.4",
        "sequelize": "^6.15.0",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.16.8",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "sequelize-cli": "^6.4.1",
        "supertest": "^6.2.2"
    }
}
